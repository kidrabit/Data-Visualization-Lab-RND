   "source": [
    "def cosine_similarity(a,b):\n",
    "    dot_product = dot(a,b)\n",
    "    \n",
    "    a_norm = linalg.norm(a)    \n",
    "    b_norm = linalg.norm(b)\n",
    "    \n",
    "    cosine_similarity = dot_product/(a_norm*b_norm)\n",
    "    return cosine_similarity"
   ]
  }
