   "outputs": [],
   "source": [
    "def overflow_softmax(x):\n",
    "    x = np.exp(x-np.max(x))\n",
    "    \n",
    "    return x/x.sum()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "eb4b7a9a",
